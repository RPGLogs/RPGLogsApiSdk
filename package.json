{
  "name": "@rpglogs/api-sdk",
  "version": "2.0.0",
  "description": "Sends HTTP requests to the RPGLogs v2 API.",
  "author": "RPGLogs LLC",
  "repository": "https://github.com/RPGLogs/RPGLogsApiSdk",
  "license": "MIT",
  "keywords": [
    "typescript",
    "rpglogs",
    "api sdk",
    "warcraft logs",
    "world of warcraft",
    "ff logs",
    "final fantasy",
    "eso logs",
    "elder scrolls online",
    "swtor logs",
    "star wars the old republic"
  ],
  "main": "./dist/tsc/main.js",
  "types": "./dist/tsc/main.d.ts",
  "browser": "./dist/esbuild/browser.js",
  "scripts": {
    "generate": "ts-node src/downloadSchema.ts && node_modules/.bin/graphql-codegen --config src/codegen.yml",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build && yarn esbuild-node && yarn esbuild-browser",
    "esbuild-node": "esbuild src/main.ts --bundle --platform=node --minify --sourcemap=external --outfile=dist/esbuild/main.js",
    "esbuild-node:dev": "esbuild src/main.ts --bundle --sourcemap=external --outfile=dist/esbuild/main.js",
    "esbuild-node:watch": "esbuild src/main.ts --bundle --watch --sourcemap=external --outfile=dist/esbuild/main.js",
    "esbuild-browser": "esbuild src/browser.ts --bundle --minify --sourcemap=external --outfile=dist/esbuild/browser.js",
    "esbuild-browser:dev": "esbuild src/browser.ts --bundle --outfile=dist/esbuild/browser.js",
    "esbuild-browser:watch": "esbuild src/browser.ts --bundle --watch --outfile=dist/esbuild/browser.js"
  },
  "dependencies": {
    "graphql": "^15.0.0",
    "graphql-request": "^6.0.0",
    "graphql-tag": "^2.12.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^1.15.1",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^1.15.1",
    "@types/btoa": "^1.2.3",
    "@types/jest": "^26.0.21",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "btoa": "^1.2.1",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.7.0",
    "prettier": "2.3.2",
    "sync-request": "^6.1.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4"
  }
}
